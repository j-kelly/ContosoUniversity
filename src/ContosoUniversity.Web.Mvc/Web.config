<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    
  <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 --></configSections>
  <connectionStrings>
    <add name="NRepository_Contoso" connectionString="Data Source=(LocalDb)\v11.0;Initial Catalog=NRepository_Contoso;Integrated Security=SSPI;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <nlog internalLogFile="c:\logs\Nlog-IIS-Internal.log" xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <variable name="brief" value="${longdate}|${logger}|${level}|${logger}|${message}" />
    <variable name="verbose" value="${longdate}|${processid}|${level}|${logger}|${message}" />
    <targets>
      <target name="f" xsi:type="File" layout="${verbose}" fileName="c:\logs\ContosoUniversity.Web.log" />
      <target name="timerFile" xsi:type="File" layout="${verbose}" fileName="c:\logs\ContosoUniversity.Timings.log" />
      <target name="AuditFile" xsi:type="File" layout="${verbose}" fileName="c:\logs\ContosoUniversity.Audit.log" />
      <target name="debug" xsi:type="Debugger" layout="${longdate} ${uppercase:${level}} ${message}" />
      <target name="console" xsi:type="ColoredConsole" layout="${brief}" />
      <target name="trace" xsi:type="File" layout="${brief}" fileName="c:\logs\ContosoUniversity.Web.trace.log" />
    </targets>
    <rules>
      <logger name="Timer" minlevel="Debug" writeTo="timerFile" />
      <logger name="Audit" minlevel="Debug" writeTo="AuditFile" />
      <logger name="ContosoUniversity.*" minlevel="Debug" writeTo="f,debug" />
    </rules>
  </nlog>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
